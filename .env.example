# ./mcp-autotask-search/.env.example
#
# Example environment configuration for Autotask Search MCP Server
# Copy this file to .env and fill in your actual values

# REQUIRED: Your Autotask API key for authentication
# Get this from your Django admin panel or API key management interface
AUTOTASK_API_KEY=your-api-key-here

# OPTIONAL: Base URL for the Autotask API
# Default: http://localhost:8000
# Change this if your Django server runs on a different host/port
AUTOTASK_API_BASE_URL=http://localhost:8000
